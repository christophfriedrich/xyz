    "fastify-auth": "^0.2.0",
    "fastify-cors": "^0.2.0",
    "fastify-formbody": "^2.0.1",
    "fastify-helmet": "^1.0.3",
    "fastify-jwt": "^0.3.0",
    "fastify-static": "^0.12.0",
    "js-datepicker": "^4.0.6",
    "jsrender": "^0.9.91",
    "leaflet": "^1.3.4",
    "leaflet.vectorgrid": "^1.3.0",
    "mobile-detect": "^1.4.3",
